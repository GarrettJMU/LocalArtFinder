<h1>Advanced Search</h1>

<%= form_for @advanced_search.artists do |f| %>

  <div class="field">
    <%= f.label :artist_name %><br />
    <%= f.text_field :artist_name %>
  </div>

  <div class="field">
    <%= f.label :price %><br />
    <%= f.text_field :price %>
  </div>

  <div class="actions">
    <%= f.submit "Search" %>
  </div>

<% end %>
