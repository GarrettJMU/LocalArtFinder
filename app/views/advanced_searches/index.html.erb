<h1>Search Results</h1>


<h1>Artists</h1>

<table>
  <thead>
    <tr>
      <th>Artist Name</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Website</th>
      <th>Sales</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr>
        <td><%= artist.artist_name %></td>
        <td><%= artist.first_name %></td>
        <td><%= artist.last_name %></td>
        <td><%= artist.email %></td>
        <td><%= artist.street %></td>
        <td><%= artist.city %></td>
        <td><%= artist.state %></td>
        <td><%= artist.zipcode %></td>
        <td><%= artist.website %></td>
        <td><%= artist.sales %></td>
        <td><%= artist.phone %></td>
        <td><%= link_to 'Show', artist %></td>
        <% if can? :update, artist %>
        <td><%= link_to 'Edit', edit_artist_path(artist) %></td>
        <td><%= link_to 'Destroy', artist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>

  </tbody>
</table>
